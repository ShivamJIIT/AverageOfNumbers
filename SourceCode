#include<bits/stdc++.h>
#include <iostream>
using namespace std;
int main()
{
    int n,no,sum;
    cout<<"Enter the size of the array:";
    cin>>n;
    vector<int> nums;
    cout<<"Enter numbers: ";
    for(int i=0;i<n;i++)
    {
        cin>>no;
        sum+=no;
    }
    float avg;
    avg=sum/n;
    cout<<"Average of the numbers: "<<avg;
    return 0;
}
